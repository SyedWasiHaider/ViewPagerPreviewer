[project]
	description = Shows a preview of offscreen fragments in a viewpager. This is highly experimental. Use at your own risk. Also, I encourage you to contribute with pull requests.
[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/SyedWasiHaider
[access "refs/for/refs/*"]
	read = group user/SyedWasiHaider
	push = group user/SyedWasiHaider
	label-Code-Review = -2..+2 group user/SyedWasiHaider
	label-Verified = -1..+1 group user/SyedWasiHaider
	rebase = group user/SyedWasiHaider
	removeReviewer = group user/SyedWasiHaider
	submit = group user/SyedWasiHaider
[access "refs/heads/*"]
	read = group user/SyedWasiHaider
	create = group user/SyedWasiHaider
	push = +force group user/SyedWasiHaider
	pushMerge = group user/SyedWasiHaider
[access "refs/tags/*"]
	push = +force group user/SyedWasiHaider
	pushTag = group user/SyedWasiHaider
	pushSignedTag = group user/SyedWasiHaider
